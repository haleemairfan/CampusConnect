{
  "expo": {
    "assets": [
      "./assets/fonts/Inter-VariableFont_slnt,wght.ttf",
      "./assets/images/icon.png",
      "./assets/images/splash.png",
      "./assets/images/adaptive-icon.png",
      "./assets/images/favicon.png",
      "./assets/fonts/Sevillana-Regular.ttf"
    ],
    "name": "CampusConnect",
    "slug": "CampusConnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F0ED"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-VariableFont_slnt,wght.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "32773eb1-92cf-4e39-a56c-df84193f5a07"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/32773eb1-92cf-4e39-a56c-df84193f5a07"
    }
  }
}
